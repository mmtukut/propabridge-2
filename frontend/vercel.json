{
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "https://propabridge-api.onrender.com/api/v1/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}
